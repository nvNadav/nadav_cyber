import sys
def common(file_name):
    dic = {}
    try:
        # Open the file in read mode
        with open(file_name, "r") as file:
            # Read the contents of the file, remove trailing newlines, and split by spaces
            words = file.read().strip("\n").split()

            # Count the occurrences of each word in the file
            for word in words:
                if word in dic:
                    dic[word] += 1  
                else:
                    dic[word] = 1
    
        # Sort the dictionary by word frequency in descending order
        sorted_dic = (sorted(dic.items(), key=lambda x: x[1], reverse=True))
        
        show(sorted_dic)

    except Exception as error:
        print (error)

def show (sorted_dic): 
        # print the first N common word
        # when N is provided in sys.argv
        
        N=int(sys.argv[1])
        x=len(sorted_dic)
        
        for i in range(N):
            x-=1
            if x<0:
                break
            print (sorted_dic[i])